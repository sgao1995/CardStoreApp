@model CardsCatalogueViewModel

<div class="row">
    <div class="card">
        <div class="card-header">
            @Model.CurrentCategory
        </div>
        <div class="card-body">
            <div class="card-deck">
                @foreach (var card in Model.Cards)
            {
                  @Html.Partial("Details", card);
                }
            </div>
        </div>
    </div>
</div>

